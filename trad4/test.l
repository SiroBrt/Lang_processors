(defun fibonacci (n) 
  (setq retorno 0) 
  (if (< n 2) 
    (setf retorno 1) 
    (setf retorno 
      (+ (fibonacci (- n 1)) (fibonacci (- n 2)))
    )
  ) 
  (+ retorno 0)
)

(defun main () 
  (setq resultado 0)
  (setq i 0)
  (print "Sucesion de Fibonacci")
  (setf i 2)
  (loop while (< i 50) do (setf resultado (fibonacci i)) (princ i) (princ resultado) (setf i (+ i 1)))
)

(main) 
